<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorEBEBF1"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context="com.tangchaoke.yiyoubangjiao.activity.Activity_AboutUs">

    <include
        android:id="@+id/include_top"
        layout="@layout/include_top" />

    <ImageView
        android:id="@+id/img_logo"
        android:layout_width="@dimen/x61"
        android:layout_height="@dimen/y43"
        android:layout_below="@id/include_top"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y10"
        android:src="@drawable/ic_launcher" />

    <TextView
        android:id="@+id/tv_about_us_version_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/img_logo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y10"
        android:text="易优帮教 V 0.0"
        android:textColor="@color/color999999"
        android:textSize="@dimen/x13" />

</RelativeLayout>
